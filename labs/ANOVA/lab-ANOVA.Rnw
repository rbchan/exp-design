<<sawData>>=
n <- length(kick.angle.brandA)
a <- 4
sawData <- data.frame(
    Kick.angle=c(kick.angle.brandA, kick.angle.brandB,
        kick.angle.brandC, kick.angle.brandD),
    Brand=rep(c("A","B","C","D"), each=n))
@
\end{frame}






