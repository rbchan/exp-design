\begin{frame}[fragile]
  \frametitle{Square Root Transformation}
%  \LARGE
  \[
  y = \sqrt{u + C}
  \]
%  \large
  \begin{itemize}%[<+->]
    \small
    \item $C$ is often 0.5 or some other small number
    \item Useful when group variances are proportional to the means
%      (count data)
  \end{itemize}
  \pause
<<sqrt,fig.width=6,fig.height=6,fig.align='center',out.width="40%",size='scriptsize'>>=
boxplot(sqrt(percentInfected)~landscape, infectionRates, col="yellow",
        cex.lab=1.5, cex.axis=1.3, ylab="sqrt(percent forest cover)")
@
\end{frame}



