\begin{frame}[fragile]
  \frametitle{Logarithmic Transformation}
%  \LARGE
  \[
  y = \log(u + C)
  \]
%  \large
  \begin{itemize}%[<+->]
    \small %\normalsize
    \item The constant $C$ is often 1, or 0 if there are no zeros in the data ($u$)
    \item Useful when group variances are proportional to the means
  \end{itemize}
  \pause
<<log,fig.width=6,fig.height=6,fig.align='center',out.width="40%",size='scriptsize'>>=
boxplot(log(percentInfected)~landscape, infectionRates, col="green",
        cex.lab=1.5, cex.axis=1.3, ylab="log(percent forest cover)")
@
\end{frame}



